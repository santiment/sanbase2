  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "src/serviceWorker.js",
      "build"
    ]
  },
  "lint-staged": {
    "linters": {
      "src/**/*.js": [
        "yarn format",
        "git add"
      ],
      "src/**/*.css": [
        "yarn test:lint:css"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
