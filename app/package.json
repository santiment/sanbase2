{
  "name": "SANbase",
  "version": "0.9.7",
  "private": true,
  "scripts": {
    "start": "BROWSER=none REACT_APP_VERSION=$(node -pe 'require(\"./package.json\").version')-$(echo $GIT_HEAD) node node_modules/exit_on_eof/index.js 'react-scripts start'",
    "start:server": "node server",
    "format": "prettier-standard 'src/**/*.js'",
    "precommit": "lint-staged",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build": "REACT_APP_VERSION=$(node -pe 'require(\"./package.json\").version')-$(echo $GIT_HEAD) react-scripts build",
    "test:lint:css": "stylelint \"**/*.css\"",
    "test:js": "react-scripts test --env=jsdom",
    "test": "npm run test:lint:css && CI=true npm run test:js",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@trainline/react-skeletor": "^1.0.2",
    "animate-components": "^1.4.8",
    "apollo-cache-inmemory": "^1.1.2",
    "apollo-client": "^2.1.0",
    "apollo-link": "^1.2.2",
    "apollo-link-context": "^1.0.2",
    "apollo-link-error": "^1.0.5",
    "apollo-link-http": "^1.3.0",
    "apollo-link-retry": "^2.2.4",
    "axios": "^0.17.1",
    "chart.js": "^2.7.1",
    "chartjs-plugin-annotation": "^0.5.7",
    "chartjs-plugin-datalabels": "^0.2.0",
    "classnames": "^2.2.5",
    "copy-to-clipboard": "^3",
    "draft-convert": "^2.0.1",
    "draftjs-md-converter": "^0.1.6",
    "font-awesome": "^4.7.0",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "medium-draft": "^0.5.6",
    "moment": "^2.19.3",
    "nprogress": "^0.2.0",
    "outliers": "^0.0.3",
    "query-string": "^5.0.1",
    "raven-for-redux": "^1.1.1",
    "raven-js": "^3.20.1",
    "react": "^16.3.0",
    "react-apollo": "^2.0.4",
    "react-chartjs-2": "^2.6.4",
    "react-dates": "16.0.1",
    "react-device-detect": "^1.4.6",
    "react-dom": "16.3.3",
    "react-fa": "^5.0.0",
    "react-form": "^2.16.1",
    "react-ga": "^2.3.5",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.3.1",
    "react-notification": "^6.8.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.4",
    "react-select": "^2.0.0-beta.2",
    "react-sizes": "^0.4.3",
    "react-stickynode": "^1.4.1",
    "react-table": "^6.7.5",
    "react-virtualized": "^9.18.5",
    "react-virtualized-select": "^3.1.3",
    "recharts": "^1.0.0-beta.10",
    "recompose": "^0.27.0",
    "redux": "^3.7.2",
    "redux-observable": "^0.18.0",
    "redux-persist": "^5.4.0",
    "rxjs": "5.x.x",
    "sanitize-html": "^1.18.2",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.82.1",
    "validate.js": "^0.12.0"
  },
  "devDependencies": {
    "@storybook/addon-viewport": "^3.3.3",
    "@storybook/react": "^3.3.3",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.2.2",
    "exit_on_eof": "^1.0.4",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "prettier-standard": "^8.0.1",
    "puppeteer": "^1.1.1",
    "redux-axios-middleware": "^4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "standard-reporter": "^1.0.5",
    "storybook-react-router": "^1.0.1",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "src/serviceWorker.js",
      "build"
    ]
  },
  "lint-staged": {
    "linters": {
      "src/**/*.{js,css}": [
        "yarn test",
        "prettier-standard",
        "git add"
      ]
    }
  }
}
